{
    "rolloutSpec": {
        "name": "rolloutspec_oceancd-kind",
        "microservice": "oleg_temp-microservice",
        "environment": "oceancd-kind-env",
        "strategy": {
            "rolling": {
                "verification": {
                    "phases": [
                        {
                            "initialDelay": "1m",
                            "name": "amir-verifications",
                            "externalVerification": {
                                "fallback": "fail",
                                "timeout": "1m"
                            }
                        }
                    ]
                }
            }
        },
        "notification": {
            "providers": [
                "amirgolan-webhook"
            ]
        },
        "failurePolicy": {
            "rollback": {
                "mode": "newRollout"
            }
        }
    }
}